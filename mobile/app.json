{
  "expo": {
    "name": "işbitir",
    "slug": "elektrikciler-mobile",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1E1B4B"
    },
    "assetBundlePatterns": [
      "**/*",
      "!assets/images/header_bg.png",
      "!assets/images/onboarding_trust_secure_neon.png",
      "!assets/images/onboarding_get_offers_neon.png"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.isbitir.app",
      "infoPlist": {
        "NSCameraUsageDescription": "Arıza fotoğrafları çekebilmeniz için kameraya erişim izni gerekiyor.",
        "NSPhotoLibraryUsageDescription": "Galeriden fotoğraf seçebilmeniz için kütüphaneye erişim izni gerekiyor.",
        "NSLocationWhenInUseUsageDescription": "Size en yakın iş ilanlarını ve ustaları göstermek için konum izni gerekiyor."
      }
    },
    "android": {
      "versionCode": 10,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1E1B4B"
      },
      "package": "com.isbitir.app",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "NOTIFICATIONS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-font",
      [
        "expo-notifications",
        {
          "color": "#6200EE",
          "sounds": [],
          "icon": "./assets/images/icon.png",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": true
        }
      ]
    ],
    "scheme": "isbitir",
    "platforms": [
      "ios",
      "android"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0a664c35-3716-4aac-9e81-995c360ccdb1"
      }
    },
    "owner": "tugce0101"
  }
}